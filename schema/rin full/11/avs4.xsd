               <xs:documentation source="ddex:Definition">A Session focusing on Artist vocals.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Demo">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Session that leads to a demo tape.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalEditing">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Session focusing on digital editing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Editing">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Session focusing on general editing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LivePerformance">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A live Performance.</xs:documentation>
            </xs:annotation>
